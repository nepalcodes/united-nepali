# Environment variables for containers, based of Apache Superset's docker setup
# https://github.com/apache/incubator-superset

COMPOSE_PROJECT=unitednepali

# database configurations (do not modify)
DATBASE_DB=unitednepali
DATBASE_DB=db
DATABASE_PASSWORD=unitednepali
DATABASE_USER=unitednepali

# database engine specific environment variable
# change according to your database engine
DATABASE_PORT=5432
DATABASE_DIALECT=postgresql
POSTGRES_DB=unitednepali
POSTGRES_USER=unitednepali
POSTGRES_PASSWORD=unitednepali

HASURA_GRAPHQL_DATABASE_URL=postgres://unitednepali:unitednepali@db:5432/unitednepali
HASURA_GRAPHQL_ENABLE_CONSOLE=true

NODE_ENV=development
